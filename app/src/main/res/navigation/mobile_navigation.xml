<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nested_navigation_list">

    <navigation
        android:id="@+id/nested_navigation_list"
        app:startDestination="@+id/navigation_diary_list_fragment">

        <fragment
            android:id="@+id/navigation_diary_list_fragment"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.DiaryListFragment"
            tools:layout="@layout/fragment_diary_list"
            android:label="@string/title_list" >

            <action
                android:id="@+id/action_navigation_diary_list_fragment_to_word_search_fragment"
                app:destination="@id/navigation_word_search_fragment"/>

            <action
                android:id="@+id/action_navigation_diary_list_fragment_to_diary_show_fragment"
                app:destination="@id/nested_navigation_diary_show">
                <argument
                    android:name="load_diary_id"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.DiaryIdUi" />
                <argument
                    android:name="load_diary_date"
                    app:argType="java.time.LocalDate" />
            </action>

            <action
                android:id="@+id/action_navigation_diary_list_fragment_to_diary_edit_fragment"
                app:destination="@id/nested_navigation_diary_edit">
                <argument
                    android:name="load_diary_id"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.DiaryIdUi"
                    app:nullable="true" />
                <argument
                    android:name="load_diary_date"
                    app:argType="java.time.LocalDate" />
            </action>

            <action
                android:id="@+id/action_diary_list_fragment_to_start_year_month_picker_dialog"
                app:destination="@id/navigation_start_year_month_picker_dialog"/>

            <action
                android:id="@+id/action_diary_list_fragment_to_diary_delete_dialog"
                app:destination="@id/navigation_diary_delete_dialog_for_diary_list_fragment"/>

            <action
                android:id="@+id/action_diary_list_fragment_to_app_message_dialog"
                app:destination="@id/navigation_app_message_dialog">
                <argument
                    android:name="app_message"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.message.AppMessage" />
            </action>

        </fragment>

        <dialog
            android:id="@+id/navigation_start_year_month_picker_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.sheet.StartYearMonthPickerDialogFragment">
            <argument
                android:name="max_year"
                app:argType="java.time.Year" />
            <argument
                android:name="min_year"
                app:argType="java.time.Year" />
        </dialog>

        <dialog
            android:id="@+id/navigation_diary_delete_dialog_for_diary_list_fragment"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.DiaryListDeleteDialogFragment">
            <argument
                android:name="date"
                app:argType="java.time.LocalDate" />
        </dialog>

        <fragment
            android:id="@+id/navigation_word_search_fragment"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.WordSearchFragment"
            tools:layout="@layout/fragment_word_search" >

            <action
                android:id="@+id/action_navigation_word_search_fragment_to_diary_show_fragment"
                app:destination="@id/nested_navigation_diary_show">
                <argument
                    android:name="load_diary_id"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.DiaryIdUi" />
                <argument
                    android:name="load_diary_date"
                    app:argType="java.time.LocalDate" />
            </action>

            <action
                android:id="@+id/action_word_search_fragment_to_app_message_dialog"
                app:destination="@id/navigation_app_message_dialog">
                <argument
                    android:name="app_message"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.message.AppMessage" />
            </action>

        </fragment>

    </navigation>

    <navigation
        android:id="@+id/nested_navigation_calendar"
        app:startDestination="@+id/navigation_calendar_fragment">

        <fragment
            android:id="@+id/navigation_calendar_fragment"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.CalendarFragment"
            tools:layout="@layout/fragment_calendar"
            android:label="@string/title_calendar" >

            <action
                android:id="@+id/action_navigation_calendar_fragment_to_diary_edit_fragment"
                app:destination="@id/nested_navigation_diary_edit">
                <argument
                    android:name="load_diary_id"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.DiaryIdUi"
                    app:nullable="true" />
                <argument
                    android:name="load_diary_date"
                    app:argType="java.time.LocalDate" />
            </action>

            <action
                android:id="@+id/action_calendar_fragment_to_app_message_dialog"
                app:destination="@id/navigation_app_message_dialog">
                <argument
                    android:name="app_message"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.message.AppMessage" />
            </action>

        </fragment>

    </navigation>

    <navigation
        android:id="@+id/nested_navigation_settings"
        app:startDestination="@+id/navigation_settings_fragment">

        <fragment
            android:id="@+id/navigation_settings_fragment"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.SettingsFragment"
            tools:layout="@layout/fragment_settings" >

            <action
                android:id="@+id/action_navigation_settings_fragment_to_theme_color_picker_dialog"
                app:destination="@id/navigation_theme_color_picker_dialog"/>

            <action
                android:id="@+id/action_navigation_settings_fragment_to_calendar_start_day_picker_dialog"
                app:destination="@id/navigation_calendar_start_day_picker_dialog"/>

            <action
                android:id="@+id/action_navigation_settings_fragment_to_reminder_notification_time_picker_dialog"
                app:destination="@id/navigation_reminder_notification_time_picker_dialog"/>

            <action
                android:id="@+id/action_settings_fragment_to_permission_dialog"
                app:destination="@id/navigation_permission_dialog"/>

            <action
                android:id="@+id/action_settings_fragment_to_all_diaries_delete_dialog"
                app:destination="@id/navigation_all_diaries_delete_dialog"/>

            <action
                android:id="@+id/action_settings_fragment_to_all_settings_initialization_dialog"
                app:destination="@id/navigation_all_settings_initialization_dialog"/>

            <action
                android:id="@+id/action_settings_fragment_to_all_data_delete_dialog"
                app:destination="@id/navigation_all_data_delete_dialog"/>

            <action
                android:id="@+id/action_settings_fragment_to_app_message_dialog"
                app:destination="@id/navigation_app_message_dialog">
                <argument
                    android:name="app_message"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.message.AppMessage" />
            </action>

            <action
                android:id="@+id/action_navigation_settings_fragment_to_open_source_licenses_fragment"
                app:destination="@id/navigation_open_source_licenses_fragment"/>

        </fragment>

        <dialog
            android:id="@+id/navigation_theme_color_picker_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.sheet.ThemeColorPickerDialogFragment">
        </dialog>

        <dialog
            android:id="@+id/navigation_calendar_start_day_picker_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.sheet.CalendarStartDayPickerDialogFragment">
            <argument
                android:name="initial_value"
                app:argType="java.time.DayOfWeek" />
        </dialog>

        <dialog
            android:id="@+id/navigation_reminder_notification_time_picker_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.picker.ReminderNotificationTimePickerDialogFragment">
        </dialog>

        <dialog
            android:id="@+id/navigation_permission_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.PermissionDialogFragment">
            <argument
                android:name="permission_name"
                app:argType="string" />
        </dialog>

        <dialog
            android:id="@+id/navigation_all_diaries_delete_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.AllDiariesDeleteDialogFragment">
        </dialog>

        <dialog
            android:id="@+id/navigation_all_settings_initialization_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.AllSettingsInitializationDialogFragment">
        </dialog>

        <dialog
            android:id="@+id/navigation_all_data_delete_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.AllDataDeleteDialogFragment">
        </dialog>

        <dialog
            android:id="@+id/navigation_open_source_licenses_fragment"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.fullscreen.OpenSourceLicensesDialogFragment" >
        </dialog>

    </navigation>

    <navigation
        android:id="@+id/nested_navigation_diary_show"
        app:startDestination="@+id/navigation_diary_show_fragment">

        <fragment
            android:id="@+id/navigation_diary_show_fragment"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.DiaryShowFragment"
            tools:layout="@layout/fragment_diary_show" >
            <argument
                android:name="load_diary_id"
                app:argType="com.websarva.wings.android.zuboradiary.ui.model.DiaryIdUi" />
            <argument
                android:name="load_diary_date"
                app:argType="java.time.LocalDate" />

            <action
                android:id="@+id/action_navigation_diary_show_fragment_to_diary_edit_fragment"
                app:destination="@id/nested_navigation_diary_edit">
                <argument
                    android:name="load_diary_id"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.DiaryIdUi"
                    app:nullable="true" />
                <argument
                    android:name="load_diary_date"
                    app:argType="java.time.LocalDate" />
            </action>

            <action
                android:id="@+id/action_diary_show_fragment_to_diary_load_failure_dialog"
                app:destination="@id/navigation_diary_load_failure_dialog_for_diary_show_fragment"/>

            <action
                android:id="@+id/action_diary_show_fragment_to_diary_delete_dialog"
                app:destination="@id/navigation_diary_delete_dialog_for_diary_show_fragment"/>

            <action
                android:id="@+id/action_diary_show_fragment_to_app_message_dialog"
                app:destination="@id/navigation_app_message_dialog">
                <argument
                    android:name="app_message"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.message.AppMessage" />
            </action>

        </fragment>

        <dialog
            android:id="@+id/navigation_diary_load_failure_dialog_for_diary_show_fragment"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.DiaryLoadFailureDialogFragment">
            <argument
                android:name="date"
                app:argType="java.time.LocalDate" />
        </dialog>

        <dialog
            android:id="@+id/navigation_diary_delete_dialog_for_diary_show_fragment"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.DiaryDeleteDialogFragment">
            <argument
                android:name="date"
                app:argType="java.time.LocalDate" />
        </dialog>

    </navigation>

    <navigation
        android:id="@+id/nested_navigation_diary_edit"
        app:startDestination="@+id/navigation_diary_edit_fragment">

        <fragment
            android:id="@+id/navigation_diary_edit_fragment"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.DiaryEditFragment"
            tools:layout="@layout/fragment_diary_edit"
            android:label="@string/title_settings" >
            <argument
                android:name="load_diary_id"
                app:argType="com.websarva.wings.android.zuboradiary.ui.model.DiaryIdUi"
                app:nullable="true" />
            <argument
                android:name="load_diary_date"
                app:argType="java.time.LocalDate" />

            <action
                android:id="@+id/action_diary_edit_fragment_to_diary_item_title_edit_dialog"
                app:destination="@id/navigation_diary_item_title_edit_dialog"/>

            <action
                android:id="@+id/action_diary_edit_fragment_to_diary_show_fragment_pop_up_to_diary_show"
                app:destination="@id/nested_navigation_diary_show"
                app:popUpTo="@id/navigation_diary_show_fragment"
                app:popUpToInclusive="true">
                <argument
                    android:name="load_diary_id"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.DiaryIdUi" />
                <argument
                    android:name="load_diary_date"
                    app:argType="java.time.LocalDate" />
            </action>

            <action
                android:id="@+id/action_diary_edit_fragment_to_diary_show_fragment_pop_up_to_diary_edit"
                app:destination="@id/nested_navigation_diary_show"
                app:popUpTo="@id/navigation_diary_edit_fragment"
                app:popUpToInclusive="true">
                <argument
                    android:name="load_diary_id"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.DiaryIdUi" />
                <argument
                    android:name="load_diary_date"
                    app:argType="java.time.LocalDate" />
            </action>

            <action
                android:id="@+id/action_diary_edit_fragment_to_diary_load_dialog"
                app:destination="@id/navigation_diary_load_dialog"/>

            <action
                android:id="@+id/action_diary_edit_fragment_to_diary_load_failure_dialog"
                app:destination="@id/navigation_diary_load_failure_dialog_for_diary_edit_fragment"/>

            <action
                android:id="@+id/action_diary_edit_fragment_to_diary_update_dialog"
                app:destination="@id/navigation_diary_update_dialog"/>

            <action
                android:id="@+id/action_diary_edit_fragment_to_diary_delete_dialog"
                app:destination="@id/navigation_diary_delete_dialog_for_diary_edit_fragment"/>

            <action
                android:id="@+id/action_diary_edit_fragment_to_date_picker_dialog"
                app:destination="@id/navigation_date_picker_dialog"/>

            <action
                android:id="@+id/action_diary_edit_fragment_to_weather_info_fetch_dialog"
                app:destination="@id/navigation_weather_info_fetch_dialog"/>

            <action
                android:id="@+id/action_diary_edit_fragment_to_diary_item_delete_dialog"
                app:destination="@id/navigation_diary_item_delete_dialog"/>

            <action
                android:id="@+id/action_diary_edit_fragment_to_diary_image_delete_dialog"
                app:destination="@id/navigation_diary_image_delete_dialog"/>

            <action
                android:id="@+id/action_diary_edit_fragment_to_exit_without_diary_save_confirmation_dialog"
                app:destination="@id/navigation_exit_without_diary_save_dialog"/>

            <action
                android:id="@+id/action_diary_edit_fragment_to_app_message_dialog"
                app:destination="@id/navigation_app_message_dialog">
                <argument
                    android:name="app_message"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.message.AppMessage" />
            </action>

        </fragment>

        <dialog
            android:id="@+id/navigation_diary_load_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.DiaryLoadDialogFragment">
            <argument
                android:name="date"
                app:argType="java.time.LocalDate" />
        </dialog>

        <dialog
            android:id="@+id/navigation_diary_load_failure_dialog_for_diary_edit_fragment"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.DiaryLoadFailureDialogFragment">
            <argument
                android:name="date"
                app:argType="java.time.LocalDate" />
        </dialog>

        <dialog
            android:id="@+id/navigation_diary_update_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.DiaryUpdateDialogFragment">
            <argument
                android:name="date"
                app:argType="java.time.LocalDate" />
        </dialog>

        <dialog
            android:id="@+id/navigation_diary_delete_dialog_for_diary_edit_fragment"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.DiaryDeleteDialogFragment">
            <argument
                android:name="date"
                app:argType="java.time.LocalDate" />
        </dialog>

        <dialog
            android:id="@+id/navigation_date_picker_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.picker.DatePickerDialogFragment">
            <argument
                android:name="date"
                app:argType="java.time.LocalDate" />
        </dialog>

        <dialog
            android:id="@+id/navigation_weather_info_fetch_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.WeatherInfoFetchDialogFragment">
            <argument
                android:name="date"
                app:argType="java.time.LocalDate" />
        </dialog>

        <dialog
            android:id="@+id/navigation_diary_item_delete_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.DiaryItemDeleteDialogFragment">
            <argument
                android:name="itemNumber"
                app:argType="integer" />
        </dialog>

        <dialog
            android:id="@+id/navigation_diary_image_delete_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.DiaryImageDeleteDialogFragment">
        </dialog>

        <dialog
            android:id="@+id/navigation_exit_without_diary_save_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.ExitWithoutDiarySaveDialogFragment">
        </dialog>

        <dialog
            android:id="@+id/navigation_diary_item_title_edit_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.fullscreen.DiaryItemTitleEditDialog"
            tools:layout="@layout/dialog_diary_item_title_edit" >
            <argument
                android:name="diary_item_title"
                app:argType="com.websarva.wings.android.zuboradiary.ui.model.DiaryItemTitle" />

            <action
                android:id="@+id/action_diary_item_title_edit_dialog_to_diary_item_title_delete_dialog"
                app:destination="@id/navigation_diary_item_title_delete_dialog"/>

            <action
                android:id="@+id/action_diary_item_title_edit_dialog_to_app_message_dialog"
                app:destination="@id/navigation_app_message_dialog">
                <argument
                    android:name="app_message"
                    app:argType="com.websarva.wings.android.zuboradiary.ui.model.message.AppMessage" />
            </action>

        </dialog>

        <dialog
            android:id="@+id/navigation_diary_item_title_delete_dialog"
            android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.DiaryItemTitleDeleteDialogFragment">
            <argument
                android:name="itemTitle"
                app:argType="string" />
        </dialog>

    </navigation>

    <dialog
        android:id="@+id/navigation_app_message_dialog"
        android:name="com.websarva.wings.android.zuboradiary.ui.fragment.dialog.alert.AppMessageDialogFragment">
        <argument
            android:name="app_message"
            app:argType="com.websarva.wings.android.zuboradiary.ui.model.message.AppMessage" />
    </dialog>

</navigation>
